{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 2], // 인덴트 설정
        "quotes": ["error", "single"], // 쿼터 dubble || single
        "semi": ["error", "always"], // semi always(default)
        "comma-style": ["error", "last"] // comma-style first || last
    }
}
